{
  "$schema": "http://json.schemastore.org/template",
  "author": "Babula",
  "classifications": [
    "Web",
    "WebAPI",
    "Simple.Modular.API"
  ],
  "identity": "Simple.Modular.API",
  "name": "Simple Modular API Project",
  "shortName": "_safeprojectname_api",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "_safeprojectname_", // replace w/ project name or -n val
  "defaultName": "_safeprojectname_api", // Visual Studio default display name
  "symbols": {
    "parameter1": {
      "type": "parameter",
      "defaultValue": "parameter 1",
      "replaces": "_parameter1_"
    }
  },
  "sources": [
    {
      "exclude": [
        "**/[Bb]in/**",
        "**/[Oo]bj/**",
        ".template.config/**/*",
        "**/*.filelist",
        "**/*.user",
        "**/*.lock.json",
        "**/ModuleTemplate/**"
      ]
    }
  ]
}